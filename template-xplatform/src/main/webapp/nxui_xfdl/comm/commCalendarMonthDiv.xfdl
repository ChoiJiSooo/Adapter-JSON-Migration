<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.1">
  <Form id="ModuleCalendarMonth" width="130" height="28" titletext="월달력" onload="form_onload" background="#ffffff" ontimer="ModuleCalendarMonth_ontimer">
